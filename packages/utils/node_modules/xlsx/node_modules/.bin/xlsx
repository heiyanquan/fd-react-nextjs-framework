#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.0@xlsx-0.20.0.tgz/node_modules/xlsx/bin/node_modules:/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.0@xlsx-0.20.0.tgz/node_modules/xlsx/node_modules:/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.0@xlsx-0.20.0.tgz/node_modules:/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.0@xlsx-0.20.0.tgz/node_modules/xlsx/bin/node_modules:/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.0@xlsx-0.20.0.tgz/node_modules/xlsx/node_modules:/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.0@xlsx-0.20.0.tgz/node_modules:/mnt/d/work/脚手架/fd-react-nextjs-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/xlsx.njs" "$@"
else
  exec node  "$basedir/../../bin/xlsx.njs" "$@"
fi
